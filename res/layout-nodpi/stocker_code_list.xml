<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
         android:orientation="vertical"
         android:layout_width="fill_parent"
         android:layout_height="fill_parent" 
     >
<LinearLayout 
    android:orientation="vertical"
    android:layout_width="443px"
    android:layout_height="443px"
    android:background="@drawable/panel"
    >
              <RelativeLayout
            android:layout_width="match_parent" 
    	    android:layout_marginTop="30px"
	        android:layout_height="100px" 
            >   
            <ImageView  
                android:id="@+id/CodelistBack"
                android:background="@drawable/btn_bg"
                android:layout_marginLeft="30px"
	            android:layout_width="60px" 
	            android:layout_height="43px" 
            /> 
             <LinearLayout
                    android:layout_width="358px"
		    android:layout_marginLeft="50px"
		    android:layout_marginTop="22px"
	    	android:layout_height="match_parent">
                <TextView  
                    android:layout_width="308px"
                    android:layout_height="wrap_content"
                    android:text="@string/stocker"
                    android:textSize="54px"
                    android:gravity="top|left"
                    android:textColor="@color/stock_text_color"
                    />
           <ImageView  
                    android:id="@+id/savebutton"
                    android:layout_marginTop="29px"
                    android:layout_width="40px"
                    android:layout_height="40px"
                    android:background="@drawable/ok_r"
                    />

                </LinearLayout> 
                </RelativeLayout>

       <LinearLayout
                   android:id="@+id/music_large_title"
                   android:layout_width="match_parent"
                   android:layout_height="6px">
            <ImageView
                    android:layout_width="6px"
                    android:layout_height="6px"
                    android:layout_marginLeft="45px"
                    android:background="@drawable/left"
                />
            <ImageView
                    android:layout_width="6px"
                    android:layout_height="6px"
                    android:layout_marginLeft="341px"
                    android:background="@drawable/right"
                />
    </LinearLayout>
      <FrameLayout    
         
		android:id="@+id/item_list"
		android:layout_width="381px"
		android:layout_height="260px"
		android:layout_marginLeft="31px"
		>
	 </FrameLayout>
	 
     
    </LinearLayout>

</LinearLayout>
